#ifndef CPP_ATMI_CONFIG_H
#define CPP_ATMI_CONFIG_H

/* autoconf section ----------------------------------------------------------------------*/
#undef CPP_ATMI_VERSION

/** catalog message set */
#define CATD_ATMI_SET 100

/* Defined if compiler support C++11 */
#undef HAVE_CPP11_MUTEX
#undef HAVE_CPP11_CHRONO
#undef HAVE_CPP11_THREAD

/* substitution lib */
#undef HAVE_CPP_PTHREAD

/* Define to 1 if you have the `pthread' library (-lpthread). */
#undef HAVE_LIBPTHREAD

/* Define to 1 if you have the `tux' library (-ltux). */
#undef HAVE_LIBTUX

/* Define to empty if `const' does not conform to ANSI C. */
#undef const

/* Define to `__inline__' or `__inline' if that's what the C compiler
   calls it, or to nothing if 'inline' is not supported under any name.  */
#ifndef __cplusplus
#undef inline
#endif


#endif
